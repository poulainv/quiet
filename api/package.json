        "@types/node-fetch": "^2.5.2",
        "cheerio": "^1.0.0-rc.3",
        "graphql": "^14.5.8",
        "graphql-yoga": "^1.18.3",
        "markdown-it": "^10.0.0",
        "nexus": "^0.12.0-beta.14",
        "nexus-prisma": "^0.5.0",
        "node-emoji": "^1.10.0",
        "node-fetch": "^2.6.0",
